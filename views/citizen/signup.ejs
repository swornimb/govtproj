</head>

<body>
    <!-- partial:index.partial.html -->
    <div class="mainContainer">
      

        <div class="">
            <div class="">
                <div class="">
                    <div class="card p-4">

                        <form id="submitForm" action="/signup" method="post" >
                            <div class="form-group required">
                                <lSabel for="username">Full Name</lSabel>
                                <input type="text" class="form-control text-lowercase" id="fullname" required=""
                                    name="fullname" value="">
                            </div>

                            <div class="form-group required">
                                <lSabel for="username">Phone Number</lSabel>
                                <input type="text" class="form-control text-lowercase" id="number" required=""
                                    name="number" value="">
                            </div>

                            <div class="form-group required">
                                <lSabel for="username">Address</lSabel>
                                <input type="text" class="form-control text-lowercase" id="address" required=""
                                    name="address" value="">
                            </div>

                           
                            <div class="form-group required">
                                <lSabel for="username">Email</lSabel>
                                <input type="text" class="form-control text-lowercase" id="email" required=""
                                    name="email" value="">
                            </div>

                            <div class="form-group required">
                                <label class="align-items-center">Password
                                    
                                <input type="password" class="form-control" required="" id="password" name="password"
                                    value="">
                            </div>
                            <div class="form-group required">
                                <label class="align-items-center">Confirm Password
                                    
                                <input type="password" class="form-control" required="" id="confirm-password" name="confirm-password"
                                    value="">
                            </div>

                            <!-- <div class="form-group mt-4 mb-4">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" id="remember-me"
                                        name="remember-me" data-parsley-multiple="remember-me">
                                    <label class="custom-control-label" for="remember-me">Remember me?</label>
                                </div>
                            </div> -->

                            <div class="form-group pt-1">
                                <button class="btn btn-primary btn-block" type="submit">Log In</button>
                            </div>
                        </form>
                        <p class="small-xl pt-3 text-center">
                            <span class="text-muted">Not a member?</span>
                            <a href="/signup">Sign up</a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
